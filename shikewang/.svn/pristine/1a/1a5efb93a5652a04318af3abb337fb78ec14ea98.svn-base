<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent"
             android:background="@drawable/register">
    <RelativeLayout
            android:id="@+id/tea_yd1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
        <LinearLayout
                android:id="@+id/rl_tillte"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/pd_tp_bm"
                >
            <RelativeLayout android:layout_width="0dp" android:layout_height="wrap_content"
                            android:layout_weight="1"
                    >
                <TextView
                        android:id="@+id/tv_tea_topic"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:paddingTop="3dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="消息"
                        android:textColor="@color/reg"
                        android:textSize="15dp"/>
                <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content"
                          android:layout_alignParentRight="true"
                          android:layout_centerVertical="true"
                          android:id="@+id/mess_num"
                          android:visibility="gone"
                          android:scaleType="centerInside"
                          android:src="@drawable/point_bg"
                        />

            </RelativeLayout>
            <TextView
                    android:id="@+id/tv_tea_mess"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingTop="3dp"
                    android:layout_marginLeft="35dp"
                    android:text="我的学生"
                    android:textColor="@color/bottom_widow_color"
                    android:textSize="15dp"/>
            <TextView
                    android:id="@+id/tv_my_stu"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingTop="3dp"
                    android:text="制作题目"
                    android:textColor="@color/bottom_widow_color"
                    android:textSize="15dp"/>
        </LinearLayout>
        <View
                android:id="@+id/tea_bac_huise"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignBottom="@+id/undine_linearLayout"
                android:layout_alignParentLeft="true"
                android:background="#578f8f8f"/>
        <LinearLayout
                android:id="@+id/undine_linearLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_tillte"
                android:layout_marginBottom="10dp"
                android:background="#CFCFCF">
            <View
                    android:id="@+id/tea_move_undline"
                    android:layout_width="wrap_content"
                    android:layout_height="5dp"
                    android:background="@color/reg"/>
        </LinearLayout>
        <LinearLayout
                android:id="@+id/tea_mid_cot"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_below="@+id/undine_linearLayout"
                android:layout_marginBottom="35dp"
                android:orientation="vertical">
        </LinearLayout>
        <include
                android:id="@+id/ic_tea_buttom"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="-258dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                layout="@layout/teather_buttom"
                />
    </RelativeLayout>
